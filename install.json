{
  "allowOnDemand": true,
  "displayName": "TCEX PYTEST",
  "dockerImage": "python:3.6.5-alpine",
  "features": [
    "aotExecutionEnabled",
    "appBuilderCompliant",
    "layoutEnabledApp",
    "secureParams"
  ],
  "languageVersion": "3.6",
  "listDelimiter": "|",
  "note": "",
  "params": [
    {
      "allowMultiple": true,
      "label": "multi choice",
      "name": "multi_choice",
      "note": "",
      "playbookDataType": [
        "String"
      ],
      "required": true,
      "sequence": 1,
      "type": "MultiChoice",
      "validValues": [
        "${TEXT}"
      ]
    },
    {
      "label": "owner",
      "name": "tc_owner",
      "note": "",
      "playbookDataType": [
        "String"
      ],
      "required": true,
      "sequence": 1,
      "type": "String",
      "validValues": [
        "${TEXT}"
      ]
    }
  ],
  "playbook": {
    "outputVariables": [
      {
        "name": "http.content",
        "type": "Binary"
      }
    ],
    "retry": {
      "allowed": true,
      "defaultDelayMinutes": 1,
      "defaultMaxRetries": 5
    },
    "type": "Utility"
  },
  "programLanguage": "PYTHON",
  "programMain": "run",
  "programVersion": "1.0.0",
  "runtimeLevel": "Playbook"
}
